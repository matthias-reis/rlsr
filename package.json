{
  "name": "rlsr",
  "version": "3.2.0",
  "description": "create npm releses and changelogs from a multi repo",
  "main": "src/index.js",
  "bin": {
    "rlsr": "./bin/rlsr.js",
    "rlsr4": "./bin/rlsr4.js"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.com/"
  },
  "scripts": {
    "c": "git-cz",
    "rlsr4": "node ./bin/rlsr4",
    "test": "yarn jest && yarn lint",
    "lint": "eslint .",
    "ts:watch": "yarn tsc --watch",
    "ts:compile": "yarn tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matthias-reis/rlsr.git"
  },
  "keywords": [
    "npm",
    "release",
    "changelog",
    "conventional-changelog"
  ],
  "author": "Matthias Reis",
  "license": "MIT",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/conventional-commits-parser": "^3.0.1",
    "@types/cosmiconfig": "^6.0.0",
    "@types/eslint": "7.2.13",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.5",
    "@types/prettier": "2.3.0",
    "@types/ramda": "^0.27.42",
    "@types/semver": "^7.3.7",
    "@types/yargs": "^17.0.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-semistandard": "^16.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^27.0.6",
    "prettier": "2.3.2",
    "semistandard": "^16.0.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@schemastore/package": "^0.0.6",
    "chalk": "^4.1.1",
    "conventional-commits-parser": "3.2.1",
    "cosmiconfig": "^7.0.0",
    "find-up": "^5.0.0",
    "git-latest-semver-tag": "1.0.2",
    "git-raw-commits": "2.0.10",
    "glob": "7.1.7",
    "lodash": "^4.17.21",
    "npmlog": "4.1.2",
    "ramda": "0.27.1",
    "recursive-diff": "^1.0.8",
    "semver": "7.3.5",
    "simple-git": "^2.40.0",
    "yargs": "^17.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "rlsr": {
    "debug": true,
    "registry": "https://nexus.dc.xing.com/repository/npm-publish/"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "<rootDir>/build"
    ]
  }
}
